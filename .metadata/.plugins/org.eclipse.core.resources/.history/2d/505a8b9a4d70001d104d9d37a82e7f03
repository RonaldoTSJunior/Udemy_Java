import java.util.Scanner;

public class entrada_dados {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String x;
		int y;
		x = sc.next();
		y = sc.nextInt();
		System.out.println("Você digitou: " + x);
		System.out.println("Você digitou: " + y);
		sc.close();
	}

}
